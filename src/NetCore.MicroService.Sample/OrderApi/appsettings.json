{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConsulSetting": {
    "ServiceName": "OrderService",
    //"ServiceIP": "localhost",
    "ServiceHealthCheck": "/healthcheck",
    //注意，docker容器内部无法使用localhost访问宿主机器，如果是控制台启动的话就用localhost
    "ConsulAddress": "http://consul:8500"
  },
  "ConnectionStrings": {
    "OrderContext": "User ID=postgres;Password=foo;Host=postgres;Port=5432;Database=Order;Pooling=true;"
  }
}
